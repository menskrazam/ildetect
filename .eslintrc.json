{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": [
        "standard"
    ],
    "parser": "@babel/eslint-parser",
    "parserOptions": {
        "requireConfigFile": false,
        "ecmaVersion": 2021
    },
    "rules": {
        "semi": 0,
        "quotes": "off",
        "no-fallthrough": "off"
    }
}
